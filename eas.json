
{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "simulator": false
      },
      "distribution": "internal",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "image": "latest"
      },
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_NEW_ARCHITECTURE": "0"
      }
    },
    "ios-test": {
      "ios": {
        "simulator": false,
        "image": "latest",
        "credentialsSource": "remote"
      },
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development",
        "EXPO_USE_HERMES": "0"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
