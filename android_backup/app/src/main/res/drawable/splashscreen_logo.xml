<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="200"
    android:viewportHeight="200">
    
    <!-- Background circle -->
    <path
        android:fillColor="#0E0E0E"
        android:pathData="M100,100 m-100,0 a100,100 0,1,1 200,0 a100,100 0,1,1 -200,0" />
    
    <!-- Grid lines -->
    <!-- Vertical lines -->
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M50,50 L50,150" />
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M75,50 L75,150" />
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M100,50 L100,150" />
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M125,50 L125,150" />
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M150,50 L150,150" />
    
    <!-- Horizontal lines -->
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M50,50 L150,50" />
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M50,75 L150,75" />
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M50,100 L150,100" />
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M50,125 L150,125" />
    <path
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M50,150 L150,150" />
    
    <!-- Concentric circles -->
    <circle
        android:cx="100"
        android:cy="100"
        android:r="40"
        android:fillColor="none"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="3" />
    
    <circle
        android:cx="100"
        android:cy="100"
        android:r="25"
        android:fillColor="none"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="3" />
    
    <circle
        android:cx="100"
        android:cy="100"
        android:r="10"
        android:fillColor="#FFFFFF" />
    
</vector>